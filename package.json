{
  "name": "local-hubspot-dev",
  "version": "1.0.0",
  "description": "Test project for local Hubspot development",
  "main": "index.js",
  "author": "Teun Rutten & Linsey Brander",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rm -rf dist && mkdir dist",
    "build": "yarn clean:dist && yarn build:gulp && yarn build:webpack",
    "build:webpack": "webpack",
    "build:gulp": "gulp build",
    "release": "standard-version && git push --follow-tags && git push && git push --tags",
    "hubspot:auth": "npx hscms auth oauth2",
    "hubspot:sync": "npx hscms sync dist public",
    "hubspot:module": "npx hscms create module",
    "deploy": "yarn build && yarn hubspot:sync"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hubspot/cms-cli": "^0.0.18",
    "babel-loader": "^8.0.6",
    "del": "^5.0.0",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-ext-replace": "^0.3.0",
    "gulp-footer": "^2.0.2",
    "gulp-hash": "^4.2.2",
    "gulp-header": "^2.0.9",
    "gulp-load-plugins": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob-import": "^0.1.0",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "node-sass": "^4.12.0",
    "require-dir": "^1.2.0",
    "standard-version": "^7.0.0",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "object-fit-images": "^3.2.4",
    "objectFitPolyfill": "^2.3.0",
    "preact": "^8.5.2",
    "raven-js": "^3.27.2",
    "smoothscroll": "^0.4.0"
  }
